numero1 = 0
numero2 = 0
pipeline{
  agent any
  stages{
    stage('Leer fichero'){
      steps{
        script{
           fichero = readFile(file: "\\numeros.txt")
            lines = fichero.readLines()
            numero1 = lines[0]
            numero2 = lines[1]
        }
      }
    }
    
  }
}
